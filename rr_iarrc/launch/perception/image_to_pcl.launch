<launch>
    <node pkg="rr_common" type="image_transform" name="image_transform" output="screen" required="true">
        <param name="transform_topics" value="/camera/lines/detection_img"/>
        <param name="camera_info_topic" value="/camera/camera_info"/>
        <param name="camera_link_name" value="camera"/>
        <param name="px_per_meter" value="100"/>
        <param name="map_dist_max" value="15"/>
        <param name="map_dist_min" value="0"/>

        <param name="fallback_fov_horizontal" value="1.6955"/>
        <param name="fallback_fov_vertical" value="1.3758"/>
        <param name="fallback_image_width" value="1280"/>
        <param name="fallback_image_height" value="964"/>
    </node>
</launch>
