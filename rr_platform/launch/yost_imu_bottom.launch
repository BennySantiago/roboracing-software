<launch>
    <!-- Bottom IMU -->
    <node pkg="rr_platform" name="imu_bottom" type="yost_imu" output="screen" >
        <param name="SERIAL_PORT" type="string" value="/dev/ttyACM0" />
        <param name="frame_id" type="string" value="imu_bottom"/>
        <param name="topic_name" type="string" value="/imu_bottom"/>
    </node>

</launch>